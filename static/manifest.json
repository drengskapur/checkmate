{
  "manifest_version": 3,
  "name": "CheckMate",
  "version": "1.0.5",
  "description": "Streamline Your Checklists and Workflows",
  "action": {
    "default_popup": "dist/popup.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "images/logo-icon.svg",
    "48": "images/logo-icon.svg",
    "128": "images/logo-icon.svg"
  },
  "background": {
    "service_worker": "dist/background.js"
  }
}